    "@types/react-dom": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.13.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "full-icu": "^1.3.4",
    "husky": "^4.3.5",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts": "^3.4.4",
    "react-styleguidist": "^11.1.5",
    "rimraf": "^3.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "sass": "^1.30.0",
